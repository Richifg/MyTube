{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-access": false,
        "jsx-boolean-value": false,
        "ban-types": false,
        "arrow-parens": [true, "ban-single-arg-parens"]
    },
    "rulesDirectory": []
}